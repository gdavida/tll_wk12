<ul class="collection with-header">
  <li class="collection-header"><h4>All Toys</h4></li>
    <% @toys.each do |t|%>
    <li class="collection-item avatar">
      <img src="<%= t.toy_image %>" alt="" class="circle">
      <span class="title"><%= link_to t.name, toy_path(t)%></span>
      <p class="secondary-content">
				<%= t.age_group.age %>
      </p>
    </li>
	<% end %>
</ul>