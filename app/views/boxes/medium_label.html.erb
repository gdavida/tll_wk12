
	<div class ="row <%= @age_group.color %> smLabel">
		<span class="col s3">
			<h2><%= @box.name %></h2>
			<h5><%= @age_group.age %></h5>
			<h5 class="">15 pieces</h5>
		</span>
		<div class="col s9 inlineBlock white">
			<% if @toy.box_id %>
				<% @toys.where(box_id:@box.id).find_each do |t|%>
				<div class="inlineBlock col s3 blackBorder smToyRow center">
					<img src="<%= t.toy_image %>" alt="" class="img120x120">
					<p class="height42 margin0"><%= t.name %> <hr><span class="borderTopBlack"><%= t.pieces %> pieces</span></p>
				</div>
				<% end %>
			<% end %>
		</div>
	</div>
