<div class="container">
<ul class="collection with-header">
        <li class="collection-item">
        	<div> <span class="fontSize17">All Locations</span>
						<%= link_to new_location_path(@location) do %>
	        		<i class="material-icons amber-text text-darken-1 secondary-content">
	        			library_add
	        		</i>
						<% end %>
        	</div>
        </li>
  <% @locations.each do |l|%>
	<li class="collection-item">
	  <div><%= link_to l.branch_name, location_path(l)%>
	    <span class="secondary-content">
			<%= l.contact_name %>
			</span>
	  </div>
	</li>
	<% end %>
</ul>
</div>